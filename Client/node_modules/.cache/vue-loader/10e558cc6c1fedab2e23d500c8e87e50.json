: {\r\n    sendMessage(messageInfo) {\r\n      //Send message\r\n      var message = {\r\n        user: this.username,\r\n        date: \"12\",\r\n        text: messageInfo,\r\n      };\r\n\r\n      this.connection.invoke(\r\n        \"sendMessage\",\r\n        this.username,\r\n        messageInfo,\r\n        this.room\r\n      );\r\n      this.messages.push(message);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}