      this.$emit(\"token\", data);\r\n        });\r\n      this.$router.push({\r\n        name: \"chatScreen\",\r\n        query: {\r\n          username: this.username,\r\n          room: this.room,\r\n        },\r\n    },\r\n  },\r\n  computed: {\r\n    goChatScreen() {\r\n      return {\r\n        name: \"chatScreen\",\r\n        query: {\r\n          username: this.username,\r\n          room: this.room,\r\n        },\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}